<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.imp.runtime.languageDescription">
      <language
            extensions="x10"
            description="X10 Concurrent Programming Language"
            natureID="x10dt.ui.launch.java.x10nature"
            icons="icons/compilationUnitNormal.gif"
            language="X10">
      </language>
   </extension>
   <extension
         point="org.eclipse.imp.runtime.parser">
      <parser
            class="x10dt.ui.parser.ParseController"
            language="X10">
      </parser>
   </extension>
   <extension
         point="org.eclipse.imp.runtime.tokenColorer">
      <tokenColorer
            class="x10dt.ui.editor.X10TokenColorer"
            language="X10">
      </tokenColorer>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="x10dt.ui.launching.X10LaunchConfigurationDelegate"
            id="x10dt.ui.launching.X10LaunchConfigurationType"
            modes="run,debug"
            name="X10 Application (Java back-end)"
            public="true"
            sourceLocatorId="org.eclipse.jdt.launching.sourceLocator.JavaSourceLookupDirector"
            sourcePathComputerId="org.eclipse.jdt.launching.sourceLookup.javaSourcePathComputer">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            icon="icons/x10_application.gif"
            configTypeID="x10dt.ui.launching.X10LaunchConfigurationType"
            id="x10dt.ui.launching.X10LaunchConfigurationTypeImage">
      </launchConfigurationTypeImage>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            type="x10dt.ui.launching.X10LaunchConfigurationType"
            class="x10dt.ui.launching.X10LaunchConfigurationTabGroup"
            id="x10dt.ui.launching.X10LaunchConfigurationTabGroup">
         <launchMode
               description="%run.launch.mode.descr"
               mode="run"
               perspective="x10dt.ui.perspective">
         </launchMode>
         <launchMode
               description="%debug.launch.mode.descr"
               mode="debug"
               perspective="x10dt.ui.perspective">
         </launchMode>
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            label="X10 Application (Java back-end)"
            icon="icons/x10_application.gif"
            modes="run,debug"
            class="x10dt.ui.launching.X10JavaLaunchShortcut"
            id="x10dt.ui.launching.x10LaunchShortcut">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <and>
                        <or>
                           <and>
                              <instanceof
                                    value="org.eclipse.core.resources.IFile">
                              </instanceof>
                              <test
                                    property="org.eclipse.debug.ui.matchesPattern"
                                    value="*.x10">
                              </test>
                           </and>
                           <instanceof
                                 value="org.eclipse.jdt.core.IPackageFragment">
                           </instanceof>
                           <instanceof
                                 value="org.eclipse.jdt.core.IPackageFragmentRoot">
                           </instanceof>
                           <instanceof
                                 value="org.eclipse.jdt.core.IJavaProject">
                           </instanceof>
                        </or>
                        <test
                              property="org.eclipse.debug.ui.projectNature"
                              value="x10dt.ui.launch.java.x10nature">
                        </test>
                     </and>
                  </iterate>
               </with>
            </enablement>
         </contextualLaunch>
         <configurationType
               id="x10dt.ui.launching.X10LaunchConfigurationType">
         </configurationType>
      </shortcut>
   </extension>
   <extension
         point="org.eclipse.imp.runtime.autoEditStrategy">
      <autoEditStrategy
            class="x10dt.ui.editor.formatting.X10AutoIndentStrategy"
            language="X10">
      </autoEditStrategy>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="x10dt.ui.launching.x10LaunchShortcut.run"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M2+M3+X X"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
      <key
            commandId="x10dt.ui.launching.x10LaunchShortcut.run"
            contextId="org.eclipse.ui.contexts.window"
            platform="carbon"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+X X">
      </key>
      <key
            commandId="x10dt.ui.launching.x10LaunchShortcut.debug"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M3+D X">
      </key>
      <key
            commandId="x10dt.ui.launching.x10LaunchShortcut.debug"
            contextId="org.eclipse.ui.contexts.window"
            platform="carbon"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+M4+D X">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            name="Run X10 Application (Java Back-End)"
            description="Run the selected main type as an X10 application"
            categoryId="org.eclipse.debug.ui.category.run"
            id="x10dt.ui.launching.x10LaunchShortcut.run">
      </command>
      <command
            name="Debug X10 Application (Java Back-End)"
            description="Run the selected main type as an X10 application"
            categoryId="org.eclipse.debug.ui.category.run"
            id="x10dt.ui.launching.x10LaunchShortcut.debug">
      </command>
   </extension>
   <extension
         point="org.eclipse.imp.runtime.outliner">
      <outliner
            class="x10dt.ui.views.Outliner"
            language="X10">
      </outliner>
   </extension>
   <extension
         point="org.eclipse.imp.runtime.foldingUpdater">
      <foldingUpdater
            class="x10dt.ui.editor.X10FoldingUpdater"
            language="X10">
      </foldingUpdater>
   </extension>
   <extension
         point="org.eclipse.imp.runtime.contentProposer">
      <proposer
            class="x10dt.ui.contentProposer.X10ContentProposer"
            language="X10">
      </proposer>
   </extension>
   <extension
         point="org.eclipse.imp.runtime.modelTreeBuilder">
      <treeBuilder
            class="x10dt.ui.editor.X10TreeModelBuilder"
            language="X10">
      </treeBuilder>
   </extension>
   <extension
         point="org.eclipse.imp.runtime.labelProvider">
      <labelProvider
            class="x10dt.ui.editor.X10LabelProvider"
            language="X10">
      </labelProvider>
   </extension>
   <extension
         id="x10dt.ui.cheatSheets"
         name="X10 Cheat Sheets"
         point="org.eclipse.ui.cheatsheets.cheatSheetContent">
      <category
            name="X10"
            id="x10dt">
      </category>
      <cheatsheet
            name="Using the X10DT"
            category="x10dt"
            contentFile="cheatsheets/UsingX10DT.xml"
            id="x10dt.ui.cheatSheets.using">
         <description>
            A brief introduction to using the X10 Development Toolkit.
         </description>
      </cheatsheet>
   </extension>
   <extension
         point="org.eclipse.imp.runtime.referenceResolvers">
      <referenceResolver
            class="x10dt.ui.editor.X10ReferenceResolver"
            language="X10">
      </referenceResolver>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="X10"
            icon="icons/x10perspective.gif"
            class="x10dt.ui.perspective.X10PerspectiveFactory"
            id="x10dt.ui.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.ui.resourcePerspective">
         <perspectiveShortcut
               id="x10dt.ui.perspective">
         </perspectiveShortcut>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <perspectiveShortcut
               id="x10dt.ui.perspective">
         </perspectiveShortcut>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.debug.ui.DebugPerspective">
         <perspectiveShortcut
               id="x10dt.ui.perspective">
         </perspectiveShortcut>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="x10dt.ui.perspective">
         <newWizardShortcut
               id="x10dt.ui.wizards.NewX10Project">
         </newWizardShortcut>
         <showInPart
               id="org.eclipse.imp.runtime.projectExplorerView">
         </showInPart>
         <showInPart
               id="org.eclipse.jdt.ui.PackageExplorer">
         </showInPart>
         <showInPart
               id="org.eclipse.ui.navigator.ProjectExplorer">
         </showInPart>
         <showInPart
               id="org.eclipse.ui.views.ResourceNavigator">
         </showInPart>
         <actionSet
               id="x10dt.ui.X10ElementCreation">
         </actionSet>
         <view
               closeable="true"
               id="x10dt.ui.astValidationView"
               minimized="false"
               relationship="stack"
               relative="org.eclipse.ui.views.ProblemView"
               showTitle="true"
               visible="false">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.imp.runtime.documentationProvider">
      <docProvider
            class="x10dt.ui.editor.X10DocProvider"
            language="X10">
      </docProvider>
   </extension>
