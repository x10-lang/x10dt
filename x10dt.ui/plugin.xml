         point="org.eclipse.ui.commands">
      <command
            name="Format"
            defaultHandler="x10dt.ui.commands.handlers.FormatCmdHandler"
            description="Format X10 source"
            id="x10dt.ui.formatCmd">
      </command>
   </extension>
   <extension
         point="org.eclipse.imp.runtime.contextHelper">
      <contextHelper
            class="x10dt.ui.help.X10ContextHelper"
            language="X10">
      </contextHelper>
   </extension>
   <extension
         point="org.eclipse.ui.editorActions">
      <editorContribution
            targetID="org.eclipse.imp.runtime.impEditor"
            id="imp.rulerActions">
         <action
               label="SelectRulerAction"
               class="x10dt.ui.editor.actions.X10SelectRulerAction"
               actionID="RulerClick"
               id="x10dt.ui.editor.actions.X10SelectRulerAction">
         </action>
      </editorContribution>
   </extension>
