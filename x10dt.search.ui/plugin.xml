<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.search.searchPages">
      <page
      		showScopeSection="true"	
            class="x10dt.search.ui.globalSearch.X10SearchPage"
            id="x10dt.search.ui.globalSearch.X10SearchPage"
            label="X10 Search">
      </page>
   </extension>
   <extension
    	id="X10SearchResultPage"
        point="org.eclipse.search.searchResultViewPages">
		<viewPage
			id="x10dt.search.ui.globalSearch.X10SearchResultPage"
			searchResultClass="x10dt.search.ui.globalSearch.X10SearchResult"
			class="x10dt.search.ui.globalSearch.X10SearchResultPage">
		</viewPage>
   </extension>

</plugin>
